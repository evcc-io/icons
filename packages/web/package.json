{
  "name": "@evcc/icons-web",
  "version": "0.3.1",
  "description": "Web Components for evcc icons",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:registry && npm run build:components",
    "build:registry": "cd ../.. && npx tsx scripts/build-package-registry.ts web",
    "build:components": "tsc",
    "clean": "rm -rf dist src/svg-registry.ts *.tsbuildinfo",
    "prepare": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "keywords": [
    "icons",
    "evcc",
    "web-components",
    "custom-elements",
    "typescript"
  ],
  "author": "naltatis, michael@evcc.io",
  "license": "CC-BY-NC-SA-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/evcc-io/evcc-icons.git",
    "directory": "packages/web"
  }
}
