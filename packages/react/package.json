{
  "name": "@evcc/icons-react",
  "version": "0.3.1",
  "description": "React components for evcc icons",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:registry && npm run build:components",
    "build:registry": "cd ../.. && npx tsx scripts/build-package-registry.ts react",
    "build:components": "tsc",
    "clean": "rm -rf dist src/svg-registry.ts *.tsbuildinfo",
    "prepare": "npm run build",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/react": "^18.2.0"
  },
  "keywords": [
    "icons",
    "evcc",
    "react",
    "components",
    "typescript"
  ],
  "author": "naltatis, michael@evcc.io",
  "license": "CC-BY-NC-SA-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/evcc-io/evcc-icons.git",
    "directory": "packages/react"
  }
}
