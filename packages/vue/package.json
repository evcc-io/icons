{
	"name": "@evcc/icons-vue",
	"version": "0.1.0",
	"description": "Vue components for evcc icons",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": ["dist", "README.md"],
	"scripts": {
		"build": "npm run clean && npm run build:components",
		"build:components": "vite build && vue-tsc --declaration --emitDeclarationOnly",
		"clean": "rm -rf dist",
		"prepare": "npm run build",
		"type-check": "vue-tsc --noEmit"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"dependencies": {
		"@evcc/icons": "file:../.."
	},
	"devDependencies": {
		"typescript": "^5.3.0",
		"vue-tsc": "^2.0.0",
		"vite": "^5.0.0",
		"@vitejs/plugin-vue": "^5.0.0",
		"vue": "^3.4.0"
	},
	"keywords": ["icons", "evcc", "vue", "components", "typescript"],
	"author": "naltatis, michael@evcc.io",
	"license": "CC-BY-NC-SA-4.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/evcc-io/evcc-icons.git",
		"directory": "packages/vue"
	}
}
